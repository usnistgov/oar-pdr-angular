{
    "compileOnSave": false,
    "compilerOptions": {
      "module": "esnext",
      "paths": {
        "oarlps": [
          "../node_modules/oarlps"
        ],
        "rxjs": [
          "./node_modules/rxjs"
        ],
        "rxjs/*": [
          "./node_modules/rxjs/*"
        ]
      },
      "outDir": "./dist/out-tsc",
      "sourceMap": true,
      "declaration": false,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "target": "ES2022",
      "typeRoots": [
        "./node_modules/@types"
      ],
      "lib": [
        "es2017",
        "dom"
      ],
      "useDefineForClassFields": false
    }
}