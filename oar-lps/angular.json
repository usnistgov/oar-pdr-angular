{
    "$schema": "../node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "oarlps": {
            "projectType": "library",
            "root": "libs/oarlps",
            "sourceRoot": "libs/oarlps/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "project": "libs/oarlps/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "libs/oarlps/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "libs/oarlps/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/oarlps/src/test.ts",
                        "tsConfig": "libs/oarlps/tsconfig.spec.json",
                        "karmaConfig": "libs/oarlps/karma.conf.js"
                    }
                }
            }
        }
    },
    "defaultProject": "oarlps"
}
