{
  "$schema": "../../node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "midas-lps": {
      "root": "",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
                "file-saver"
            ],
            "outputPath": "dist/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              {
                "glob": "**/*",
                "input": "../../node_modules/oarng/src/assets/",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "../../node_modules/oarlps/src/assets/",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "src/assets/",
                "output": "/assets"
              }
            ],
            "styles": [
              "src/styles.scss",
              "../../node_modules/primeng/resources/primeng.css",
              "../../node_modules/primeicons/primeicons.css",
              "../../node_modules/primeflex/primeflex.css",
              "../../node_modules/primeng/resources/themes/saga-blue/theme.css",
              "src/styles/font-awesome/css/font-awesome.css",
              "../../styles/fontawesome-free-5.15.4-web/css/all.css",
              "../../node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles/main.css",
              "src/styles/fontawesome-free-5.15.4-web/css/all.css",
              "src/app/content/modal.less",
              "../../node_modules/ngx-toastr/toastr.css"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "midas-lps:build",
            "port": 4202    
          },
          "configurations": {
            "production": {
              "browserTarget": "midas-lps:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "midas-lps:build"
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/server",
            "main": "server.ts",
            "tsConfig": "src/tsconfig.server.json",
            "sourceMap": true,
            "optimization": false,
            "buildOptimizer": false
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            ,
            "optimization": true,
            "buildOptimizer": true
}
          },
          "defaultConfiguration": ""
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "midas-lps:build:development",
              "serverTarget": "midas-lps:server:development"
            },
            "production": {
              "browserTarget": "midas-lps:build:production",
              "serverTarget": "midas-lps:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "midas-lps:build:production",
              "serverTarget": "midas-lps:server:production"
            },
            "development": {
              "browserTarget": "midas-lps:build:development",
              "serverTarget": "midas-lps:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "midas-lps-e2e": {
      "root": "e2e",
      "projectType": "application",
      "cli": {},
      "schematics": {},
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "midas-lps:serve"
          }
        }
      }
    }
  },
  "cli": {},
  "schematics": {
    "@schematics/angular:class": {
      "skipTests": false
    },
    "@schematics/angular:component": {
      "skipTests": false,
      "inlineStyle": false,
      "inlineTemplate": false,
      "prefix": "app",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "skipTests": false,
      "prefix": "app"
    },
    "@schematics/angular:guard": {
      "skipTests": false
    },
    "@schematics/angular:module": {
    },
    "@schematics/angular:pipe": {
      "skipTests": false
    },
    "@schematics/angular:service": {
      "skipTests": false
    }
  }
}